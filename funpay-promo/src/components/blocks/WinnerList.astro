---
interface Props {
    items?: Array<string>;
    class?: string;
    [x: string]: any;
}

const {
    items = [],
    class: className,
    ...rest
} = Astro.props;
---
<div class="lootery-modal__winners">
    {items.map((item) => (
        <div class="lootery-modal__winner">{item}</div>
    ))}
</div>
