{
  "extends": "astro/tsconfigs/base",

  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data/*": ["data/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@scripts/*": ["src/scripts/*"],
      "@nodeModules/*": ["node_modules/*"]
    }
  }
}
